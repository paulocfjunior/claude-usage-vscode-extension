{
  "permissions": {
    "allow": [
      "Read(*)",
      "Edit(*)",
      "Write(*)",
      "Bash(*)",
      "WebFetch(*)",
      "WebSearch(*)",
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "Fetch(*)",
      "Task(*)",
      "WebFetch(domain:raw.githubusercontent.com)",
      "Bash(git -C /Users/paulocfjunior/code/clever/claude-usage-vscode-extension log --oneline -5)",
      "Bash(npm install:*)",
      "Bash(npm run compile:*)"
    ],
    "deny": [
      "Read(**/.env*)",
      "Read(**/secrets/**)",
      "Read(**/*.pem)",
      "Read(**/*.key)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)"
    ],
    "ask": []
  }
}
